Login: /api/user/login --POST
Register: /api/user --POST  -username -password -firstName -lastName -uploadFile -aboutMe
Add Education: /api/education --POST --school, --degree, --field, --startDate, --endDate, --description
Delete Education: /api/education/:id --id will be education id DELETE REQ
Edit Education: /api/education/:id --id will be education id PUT REQ --school, --degree, --field, --startDate, --endDate, --description

